<div data-proportion-id=<%= proportion.id %>>
  <li><%=  proportion.material.name %> - <%=  proportion.percentage %> %</li>
  <%= link_to "delete",
        proportion_path(proportion),
        method: :delete,
        data: { confirm: "Are you sure?" },
        remote: true,
        class: "delete-button" %>
</div>
